{
  "hosting": {
    "public": "./",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "payments"
      }
    ]
  },
  "functions": [
    {
      "source": "./",
      "codebase": "default"
    }
  ]
}
